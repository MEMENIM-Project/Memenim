<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Memenim.Layouts.NavigationBar"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <system:String x:Key="ResourceDictionaryName">layout-NavigationBar-NavBarNone</system:String>

    <x:Array x:Key="TriggeredOnPages"
             Type="system:String">
        <system:String>LoginPage</system:String>
        <system:String>RegisterPage</system:String>
    </x:Array>

    <x:Array x:Key="TopNavButtons"
             Type="local:NavRedirectButtonNode"/>
    <x:Array x:Key="CentralNavButtons"
             Type="local:NavRedirectButtonNode"/>
    <x:Array x:Key="BottomNavButtons"
             Type="local:NavRedirectButtonNode"/>
</ResourceDictionary>