<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Memenim.TabLayouts.NavigationBar"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <system:String x:Key="ResourceDictionaryName">layout-NavigationBar-NavBarDefault</system:String>

    <x:Array x:Key="TriggeredOnPages" Type="system:String">
        <system:String>FeedPage</system:String>
        <system:String>SubmitPostPage</system:String>
        <system:String>PlaceholderPage</system:String>
        <system:String>SettingsPage</system:String>
        <system:String>UserProfilePage</system:String>
        <system:String>MemesPage</system:String>
        <system:String>PostOverlayPage</system:String>
    </x:Array>

    <x:Array x:Key="TopNavButtons" Type="local:NavRedirectButtonNode">
        <local:NavRedirectButtonNode PageName="Back" IconKind="ArrowLeft"/>
    </x:Array>
    <x:Array x:Key="CentralNavButtons" Type="local:NavRedirectButtonNode">
        <local:NavRedirectButtonNode PageName="PlaceholderPage" IconKind="Magnify"/>
        <local:NavRedirectButtonNode PageName="FeedPage" IconKind="Newspaper"/>
        <local:NavRedirectButtonNode PageName="PlaceholderPage" IconKind="Chat"/>
    </x:Array>
    <x:Array x:Key="BottomNavButtons" Type="local:NavRedirectButtonNode">
        <local:NavRedirectButtonNode PageName="SettingsPage" IconKind="Settings"/>
    </x:Array>
</ResourceDictionary>