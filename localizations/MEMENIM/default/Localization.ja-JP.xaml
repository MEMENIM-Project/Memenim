<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <system:String x:Key="ResourceDictionaryName">localization-xaml</system:String>

    <system:String x:Key="Login">ユーザー名前</system:String>
    <system:String x:Key="Password">パスワード</system:String>
    <system:String x:Key="RememberMe">ユーザーを覚えている</system:String>

    <system:String x:Key="FeedTitle">フィードー</system:String>

    <system:String x:Key="LanguageSettingsTitle">言語</system:String>
    <system:String x:Key="SignOut">サインアウト</system:String>

    <system:String x:Key="ErrorTitle">エーラー</system:String>

</ResourceDictionary>